{
  "kafka": {
    "brokersList": "",
    "consumer": {
      "consumerGroupId": "MtsCommandHandlerGroup",
      "clientId": "consumer-",
      "pollTimeout": 10000
    },
    "producer": {
      "clientId": "mts-command-handler-producer"
    },
    "environment": "dev" 
  },

  "cassandra": {
    "contactPoints": "caskaf01-petproject.westeurope.cloudapp.azure.com,caskaf02-petproject.westeurope.cloudapp.azure.com,caskaf03-petproject.westeurope.cloudapp.azure.com",
    "keyspace": "localhost_svc_transactions_writemodel",
    "replicationParameters": "{ \"class\": \"NetworkTopologyStrategy\", \"dc1\": 2 }",
    "consistencyLevel": {
      "write": "LocalQuorum",
      "read": "LocalOne"
    }
  },

  "ConsulStoreConfiguration": {
    "Platform": "micro-transactions",
    "Environment": "localhost",
    "ServiceName": "mts-command-handler"
  },

  "ConsulClientConfiguration": {
    "Address": "http://consul01-petprojects.westeurope.cloudapp.azure.com:8500",
    "ClientTimeoutMs": 15000000
  },

  "ConsulWatcherConfiguration": {
    "BlockingQueryTimeoutMs": 10000000,
    "DelayBetweenFailedRequestsMs": 2000
  },

  "logging": {
    "logLevel": "Verbose",
    "periodMs": 10,
    "batchSizeLimit": 50,
    "logType": "MtsCommandHandler",
    "kafka": {
      "brokersList": "",
      "topic": "applicationlogs-dev"
    }
  }
}
